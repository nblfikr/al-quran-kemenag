[
    {
        "ayat": 1,
        "lafal": "\u0627\u064e\u0644\u064e\u0645\u0652 \u0646\u064e\u0634\u0652\u0631\u064e\u062d\u0652 \u0644\u064e\u0643\u064e \u0635\u064e\u062f\u0652\u0631\u064e\u0643\u064e\u06d9",
        "terjemah": "Bukankah Kami telah melapangkan dadamu (Muhammad)?"
    },
    {
        "ayat": 2,
        "lafal": "\u0648\u064e\u0648\u064e\u0636\u064e\u0639\u0652\u0646\u064e\u0627 \u0639\u064e\u0646\u0652\u0643\u064e \u0648\u0650\u0632\u0652\u0631\u064e\u0643\u064e\u06d9",
        "terjemah": "dan Kami pun telah menurunkan bebanmu darimu,"
    },
    {
        "ayat": 3,
        "lafal": "\u0627\u0644\u0651\u064e\u0630\u0650\u064a\u0652\u0653 \u0627\u064e\u0646\u0652\u0642\u064e\u0636\u064e \u0638\u064e\u0647\u0652\u0631\u064e\u0643\u064e\u06d9",
        "terjemah": "yang memberatkan punggungmu, "
    },
    {
        "ayat": 4,
        "lafal": "\u0648\u064e\u0631\u064e\u0641\u064e\u0639\u0652\u0646\u064e\u0627 \u0644\u064e\u0643\u064e \u0630\u0650\u0643\u0652\u0631\u064e\u0643\u064e\u06d7",
        "terjemah": "dan Kami tinggikan sebutan (nama)mu bagimu."
    },
    {
        "ayat": 5,
        "lafal": "\u0641\u064e\u0627\u0650\u0646\u0651\u064e \u0645\u064e\u0639\u064e \u0627\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627\u06d9",
        "terjemah": "Maka sesungguhnya beserta kesulitan ada kemudahan,"
    },
    {
        "ayat": 6,
        "lafal": "\u0627\u0650\u0646\u0651\u064e \u0645\u064e\u0639\u064e \u0627\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627\u06d7",
        "terjemah": "sesungguhnya beserta kesulitan itu ada kemudahan."
    },
    {
        "ayat": 7,
        "lafal": "\u0641\u064e\u0627\u0650\u0630\u064e\u0627 \u0641\u064e\u0631\u064e\u063a\u0652\u062a\u064e \u0641\u064e\u0627\u0646\u0652\u0635\u064e\u0628\u0652\u06d9",
        "terjemah": "Maka apabila engkau telah selesai (dari sesuatu urusan), tetaplah bekerja keras (untuk urusan yang lain),"
    },
    {
        "ayat": 8,
        "lafal": "\u0648\u064e\u0627\u0650\u0644\u0670\u0649 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e\u0627\u0631\u0652\u063a\u064e\u0628\u0652 \u08d6",
        "terjemah": "dan hanya kepada Tuhanmulah engkau berharap."
    }
]