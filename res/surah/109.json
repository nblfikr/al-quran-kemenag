    {
        "ayat": 4,
        "lafal": "\u0648\u064e\u0644\u064e\u0627\u0653 \u0627\u064e\u0646\u064e\u0627\u06e0 \u0639\u064e\u0627\u0628\u0650\u062f\u064c \u0645\u0651\u064e\u0627 \u0639\u064e\u0628\u064e\u062f\u0652\u062a\u0651\u064f\u0645\u0652\u06d9",
