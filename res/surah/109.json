        "lafal": "\u0648\u064e\u0644\u064e\u0627\u0653 \u0627\u064e\u0646\u0652\u062a\u064f\u0645\u0652 \u0639\u0670\u0628\u0650\u062f\u064f\u0648\u0652\u0646\u064e \u0645\u064e\u0627\u0653 \u0627\u064e\u0639\u0652\u0628\u064f\u062f\u064f\u06da",
