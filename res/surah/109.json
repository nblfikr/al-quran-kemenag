[
    {
        "ayat": 1,
        "lafal": "\u0642\u064f\u0644\u0652 \u064a\u0670\u0653\u0627\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0627\u0644\u0652\u0643\u0670\u0641\u0650\u0631\u064f\u0648\u0652\u0646\u064e\u06d9",
        "terjemah": "Katakanlah (Muhammad), \u201cWahai orang-orang kafir!"
    },
    {
        "ayat": 2,
        "lafal": "\u0644\u064e\u0627\u0653 \u0627\u064e\u0639\u0652\u0628\u064f\u062f\u064f \u0645\u064e\u0627 \u062a\u064e\u0639\u0652\u0628\u064f\u062f\u064f\u0648\u0652\u0646\u064e\u06d9",
